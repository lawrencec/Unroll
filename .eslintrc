{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "immutable/no-let": 2,
    "immutable/no-this": 2,
    "immutable/no-mutation": 2
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "plugins": [
    "immutable"
  ],
  "globals": {
    "window": true,
    "describe": true,
    "expect",
    "it": true,
    "beforeEach": true,
    "afterEach": true,
    "test": true,
    "suite": true,
    "jasmine": true
  }
}
